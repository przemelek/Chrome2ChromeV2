{
  "manifest_version": 2,
  "name": "Chrome2ChromeV2",
  "version": "0.0.4",
  "description": "Send page from Chrome to Chrome on another machine",
  "browser_action": {
    "default_popup": "chrome2chrome.html",
    "default_icon": "chrome2chrome.png"
  },
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}
